class employee:
    def __init__(self):
        self.name = input("Enter name: ")
        self.age = input("Enter age: ")
        self.id = input("Enter id: ")

    def display(self):
        print("name is: ", self.name)
        print("age is: ", self.age)
        print("id is: ", self.id)
obj = employee()
obj.display()